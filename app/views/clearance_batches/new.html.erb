
<div id="wrapper">
  <div class="col-sm-12">
    <div class="searchbar">
      <%= form_tag(potential_clearance_item_path, role: "form") do %>
        <div class="input-group add-on">
          <%= text_field_tag "itemID", "", placeholder: "Enter Item Number", class: "form-control" %>
            <div class="input-group-btn">
              <%= button_tag(type: "submit", class: "btn btn-default") do %>
                <i class="fa fa-search"></i>
              <% end %>
            </div>
        </div>
      <% end %>
    </div>
  </div>
</div>

<div class="displayItemWrapper">
  <table id="itemsTable" class="table table-hover">
    <thead>
      <tr>
        <th>Item # </th>
        <th>Name</th>
        <th>Status</th>
        <th>Wholesale Price, $</th>
        <th>Price Sold, $</th>
      </tr>
    </thead>
    <tbody></tbody>
  </table>
</div>
